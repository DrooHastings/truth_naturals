/** Collection Page **/

.youtube-player .play {
  height: 50px;
  width: 78px;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  position: absolute;
  background: url({{ 'video_bg.png' | asset_url }}) no-repeat;
  cursor: pointer;
}

.custom-page-width{
  width: 100%;
  max-width: 1240px;
  margin: 0 auto;
}
.accordion {
  cursor: pointer;
  padding: 18px;
  padding-right: 30px;
  width: 100%;
  text-align: left;
  border: none;
  outline: none;
  transition: 0.4s;
  position: relative;
  margin-bottom: 0;
  margin-top: 5px;
}
.accordion:first-of-type{
  margin-top: 0;
}

.accordion.active .chevron::before{
  transform: rotate(-45deg);
}

.accordion-panel {
  padding: 5px 18px;
  background-color: white;
  display: none;
  overflow: hidden;
  //margin-bottom: 1.66667em;
}
.accordion-panel.active{
  display: block;
}
.chevron{
  position: absolute;
  right: 10px;
  top: 50%;
  transform: translateY(-50%);
}
.chevron::before {
  border-style: solid;
  border-width: 0.25em 0.25em 0 0;
  content: '';
  display: inline-block;
  height: 12px;
  position: relative;
  vertical-align: top;
  width: 12px;
  transform: rotate(135deg);
  transition: all 0.3s ease;
}

.cc-flex-container{
  display: flex;
  justify-content: space-between;
}
.cc-twi-flex-container{
  padding: 30px 0;
}
.cc-image-block{
  width: 32%;
