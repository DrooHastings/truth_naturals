    opacity: 1;
    .ccnt-add-to-cart.purchase {
    	z-index: 3;
    }
  }
}
.ccn-product-image-link {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
}
