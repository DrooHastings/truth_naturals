{% assign zp_use_favicon = false %}{% assign zp_use_meta_tags = false %}{% assign zp_use_open_graph_tags = false %}<!doctype html>
<!--[if IE 9]> <html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->

<head>

  <!-- Blockshop Theme - Version 4.0.6 by Troop Themes - http://troopthemes.com/ -->
  {% include 'framework--version' %}

  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta content="origin" id="mref" name="referrer">

  {% if settings.favicon != blank %}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {% endif %}

  {% if template == 'index' %}
    <title>{{ page_title }}</title>
  {% elsif template == 'password' %}
    <title>{{ shop.name }}</title>
  {% else %}
    <title>{{ page_title }} | {{ shop.name }}</title>
  {% endif %}

  {% if page_description %}
  <meta name="description" content="{{ page_description }}" />
  {% endif %}

  <link rel="canonical" href="{{ canonical_url }}" />

  {% include 'advanced-settings' with 'variables' %}
  {% include 'snippet-fb-open-graph-tags' %}
  {% include 'snippet-webfont' %}

  {% comment %}{{ content_for_header }}{% endcomment %}{% capture zp_include_html %}{% include 'header-scripts.zipifypages' %}{% endcapture %}{% unless zp_include_html contains 'Liquid error: Could not find asset snippets/header-scripts.zipifypages.liquid' or zp_include_html contains 'Liquid error: Could not find asset snippets/page-content-parser.zipifypages.liquid' %}{{ zp_include_html | strip }}{% assign zp_include_html = '' %}{% endunless %}

  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
  {{ 'theme.scss.css' | asset_url | stylesheet_tag }}
  {{ 'custom.scss.css' | asset_url | stylesheet_tag }}
  {{ 'swiper.css' | asset_url | stylesheet_tag }}
  {% include 'advanced-settings' with 'styles' %}
  {% include 'custom-styles' %}

  <!--[if IE]>
    <style>
      .main-header .currency-selector select {
        width: 80px;
        padding-right: 10px;
      }
    </style>
  <![endif]-->

  <!-- >= IE 11 -->
  <style>
    @media screen and (-ms-high-contrast: active), (-ms-high-contrast: none) {
      .main-header .currency-selector select {
        width: 80px;
        padding-right: 10px;
      }
    }
  </style>

  {{ '//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js' | script_tag }}
  {{ '//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js' | script_tag }}
  {{ 'api.jquery.js' | shopify_asset_url | script_tag }}
  {{ 'option_selection.js' | shopify_asset_url | script_tag }}
  {{ 'swiper.min.js' | asset_url | script_tag }}

  {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}

  <script>
    var twitterID = '{{ settings.twitter_username }}',
    general_external_links_enabled = true,
    general_scroll_to_active_item = true,
    shop_url = '{{ shop.url }}',
    shop_money_format = '{{ shop.money_format | strip_html | replace: "'", '"' | strip }}',
    currency_switcher_enabled = {{ settings.show_multiple_currencies }},
    popup_config = {
      enabled               : {{ settings.popup--enabled }},
      test_mode             : {{ settings.popup--test-mode }},
      newsletter_enabled    : {{ settings.popup--newsletter--enabled }},
      social_icons_enabled  : {{ settings.popup--social-icons--enabled }},
      show_image_enabled    : '{{ settings.popup--image.size }}',
      image_link            : "{{ settings.popup--image-url }}",
      seconds_until         : {{ settings.popup--seconds-range }},

      {% comment %} So it doesnt get picked up by og:image {% endcomment %}
      {% if settings.popup--image %}
      show_image_url        : '{{ settings.popup--image | img_url: "1200x" | img_tag: settings.popup--image.alt | default: "" }}',
      {% endif %}

      page_content          : '{{ settings.popup--page-content }}',
      days_until            : '{{ settings.popup--days-range }}',
      storage_key           : 'troop_popup_storage'
    };

    popup_config.show_image_enabled = popup_config.show_image_enabled > 0 ?
                                      true :
                                      false;

    // newer naming convention
    var theme = {};
    theme.settings = {};
    theme.translation = {}; // refactor to pluralised
    theme.translations = {};
    theme.classes = {};
    theme.utils = {};
    theme.templates = {};
    theme.partials = {};
    theme.shop_currency = '{{ shop.currency }}';

    theme.translation.newsletter_success_message = '{{ "layout.footer.newsletter_success_message" | t }}';
    theme.translation.newsletter_email_blank = '{{ "layout.footer.newsletter_email_blank" | t }}';
    theme.translations.general_search_see_all_results = '{{ "general.search.see_all_results" | t }}';
    theme.preload_image = '{{ "popup.jpg" | asset_url }}';

  </script>

  {% if settings.show_multiple_currencies %}
    {% include 'snippet-currencies' %}
  {% endif %}

  {{ 'theme.js' | asset_url | script_tag }}

</head>

<body class="page-{{ page_title | handle }} {% if customer %}customer-logged-in {% endif %}template-{{ template | split: '.' | first | remove: '/' | remove: '_' }}">

  {% include 'icon-sprite.svg' %}

  <div
    class="off-canvas--viewport"
    data-js-class="FrameworkOffCanvas"
    data-off-canvas--state="closed"
  >
    <div class="off-canvas--overlay"></div>
    <div class="off-canvas--close">{% include 'framework--icon', icon: 'cross' %}</div>

    <div class="off-canvas--left-sidebar">
      {% include 'partial--mobile-nav' %}
    </div>

    <div class="off-canvas--main-content">
      
      
      {% comment %}
      {% section 'custom-header' %}
      {% endcomment %}
      
      {% section 'rebrand-header' %}

      {% comment %}<a href="#" class="manual-optin-trigger" data-optin-slug="{{ settings.theme_optin }}">Click Me!</a>	{% endcomment %}


      <section class="main-content">
        {% comment %}{{ content_for_layout }}{% endcomment %}{% capture zp_include_html %}{% include 'page-content.zipifypages' %}{% endcapture %}{% if zp_include_html contains 'Liquid error: Could not find asset snippets/page-content.zipifypages.liquid' %}{{ page.content | strip }}{% else %}{{ zp_include_html | strip }}{% endif %}{% assign zp_include_html =  %}
      </section>

      {% section 'footer-2' %}

      {% unless template contains 'password' %}
        <button class="popup-modal-close">&#x2715;</button>
        <div class="popup-modal-mask"></div>
      {% endunless %}

      <!--<style> iframe#admin_bar_iframe { display: none !important; } </style>-->
    </div>

    <div class="off-canvas--right-sidebar">
      <div class="off-canvas--close">{% include 'framework--icon', icon: 'cross' %}</div>
    </div>

  </div>

  <script>
    document.addEventListener("DOMContentLoaded",
                              function() {
      var div, n,
          v = document.getElementsByClassName("youtube-player");
      for (n = 0; n < v.length; n++) {
        div = document.createElement("div");
        div.setAttribute("data-id", v[n].dataset.id);
        div.innerHTML = labnolThumb(v[n].dataset.id);
        div.onclick = labnolIframe;
        v[n].appendChild(div);
      }
    });

    function labnolThumb(id) {
      var thumb = '<img src="https://i.ytimg.com/vi/ID/hqdefault.jpg">',
          play = '<div class="play"></div>';
      return thumb.replace("ID", id) + play;
    }

    function labnolIframe() {
      var iframe = document.createElement("iframe");
      var embed = "https://www.youtube.com/embed/ID?rel=0&amp;controls=0&amp;showinfo=0";
      iframe.setAttribute("src", embed.replace("ID", this.dataset.id));
      iframe.setAttribute("frameborder", "0");
      iframe.setAttribute("allowfullscreen", "1");
      $(this).hide();
      this.parentNode.append(iframe);
      //this.parentNode.replaceChild(iframe, this);
    }
    document.addEventListener("DOMContentLoaded", function(event) {

    $('.accordion').on('click', function(){
      $('.accordion').removeClass('active');
      panel = $(this).next();
      if (panel.hasClass('active')){
        panel.removeClass('active');
      } else {
        $(this).addClass('active');
        $('.accordion-panel').removeClass('active')
        panel.addClass('active');
          $('html, body').animate({
            scrollTop: $('.accordion.active').offset().top - 100
          }, 300);
      }
    });

  });

  </script>

{% capture zp_include_html %}{% include 'page-footer.zipifypages' %}{% endcapture %}{% unless zp_include_html contains 'Liquid error: Could not find asset snippets/page-footer.zipifypages.liquid' %}{{ zp_include_html | strip }}{% assign zp_include_html = '' %}{% endunless %}</body>
</html>

{% layout none %}
