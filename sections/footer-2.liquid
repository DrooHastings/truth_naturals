                        </defs>
                        <g id="Origins-AUS" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                            <g id="1d-Home" transform="translate(-1146.000000, -6634.000000)">
                                <g id="Group-8" transform="translate(876.000000, 6596.000000)">
                                    <g id="Group-2-Copy" transform="translate(270.000000, 38.000000)">
                                        <g id="icon/fontawesome/search">
                                            <g id="color/3BB44A" transform="translate(0.000000, -1.000000)">
