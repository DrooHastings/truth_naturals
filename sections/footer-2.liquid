{% comment %}<footer id="ftr-sctn--wrap">
  <div id="ftr-sctn--content-wrap">
    <div id="ftr-sctn--content">
      {% if section.settings.logo_mob %}
        <div id="ftr--logo-wrp">
          <a href="/" title="{{ shop.name }}" id="ftr--logo">
            <picture id="ftr--logo-pctr">
              {% if section.settings.logo_desk %}
                <source media="(min-width: 768px)" srcset="{{ section.settings.logo_desk | img_url: 'master' }}">
              {% endif %}
              <img src="{{ section.settings.logo_mob | img_url: 'master' }}" alt="{{ shop.name }}" id="ftr--logo-img">
            </picture>
          </a>
        </div>
      {% else %}
        {{ shop.name }}
      {% endif %}

      {% assign k = true %}

      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'menu' %}

            {% if k %}
              <div id="ftr-sctn--mn-wrp">
            {% endif %}
            {% assign k = false %}

            {% if block.settings.linklist != blank %}
              <div class="ftr-sctn--cntnr">
                <h6 class="ftr-sctn--mn-ttl">{{ linklists[block.settings.linklist].title }}</h6>
                <ul class="ftr-sctn--mn-list">
                  {% for link in linklists[block.settings.linklist].links %}
                    <li class="ftr-sctn--mn-item">
                      <a href="{{ link.url }}" title="{{ link.title }}" class="ftr-sctn--mn-lnk">{{ link.title }}</a>
                    </li>
                  {% endfor %}
                </ul>
              </div>
            {% endif %}

          {% when 'menu_with_info' %}
            <div class="ftr-sctn--cntnr ftr-sctn--cntnr-info">
              {% if block.settings.linklist != blank %}
                <h6 class="ftr-sctn--mn-ttl">{{ linklists[block.settings.linklist].title }}</h6>
                <ul class="ftr-sctn--mn-list">
                  {% for link in linklists[block.settings.linklist].links %}
                    <li class="ftr-sctn--mn-item">
                      <a href="{{ link.url }}" title="{{ link.title }}" class="ftr-sctn--mn-lnk">{{ link.title }}</a>
                    </li>
                  {% endfor %}
                </ul>
              {% endif %}
              <div id="ftr-sctn--inf">
                {% if block.settings.phone_number != empty %}
                  <div class="ftr-sctn--inf-itm">
                    <a href="tel:{{ block.settings.country_phone_code }}{{ block.settings.phone_number | remove: ' ' }}" target="_blank" id="ftr-sctn--inf-ph" class="ftr-sctn--inf-lnk">
                      <span class="ftr-sctn--inf-txt">{{ block.settings.phone_number }}</span>
                    </a>
                  </div>
                {% endif %}

                {% if block.settings.email != empty %}
                  <div class="ftr-sctn--inf-itm">
                    <a href="mailto:{{ block.settings.email }}" target="_blank" id="ftr-sctn--inf-eml" class="ftr-sctn--inf-lnk">
                      <span class="ftr-sctn--inf-txt">{{ block.settings.email }}</span>
                    </a>
                  </div>
                {% endif %}

                {% if block.settings.address_url != empty %}
                  <div class="ftr-sctn--inf-itm">
                    <a href="{{ block.settings.address_url }}" id="ftr-sctn--inf-adrs" class="ftr-sctn--inf-lnk">
                      <span class="ftr-sctn--inf-txt">{{ block.settings.address }}</span>
                    </a>
                  </div>
                {% endif %}
              </div>
            </div>
            </div>

          {% when 'subscribe_and_search' %}

            <div class="ftr-sbscrptn-and-srch">
              <div id="ftr-sctn--sbscrptn">
                {% if block.settings.sbscrptn_txt != empty %}
                  <a class="show-subscription-popup manual-optin-trigger" id="ftr--sbscrptn-btn" href="{{ settings.theme_optin }}" target="_blank" data-optin-slug="{{ settings.theme_optin }}">{{ block.settings.sbscrptn_txt }}</a>
                {% endif %}
              </div>

              <div id="ftr-sctn--srch">
                {% if block.settings.srch_title != empty %}
                  <h6 class="ftr-sctn--mn-ttl" id="ftr-sctn--srch-ttl">{{ block.settings.srch_title }}</h6>
                {% endif %}

                <form class='ftr-search' action="/search" method="get" role="search" id="ftr--srch-frm">
                  <input name="q" id="ftr--srch-fld" type="text" placeholder="{{ 'layout.header.search_placeholder' | t }}"/>
                  <button id="ftr--srch-btn">
                    <?xml version="1.0" encoding="UTF-8"?>
                    <svg id="search-icon" width="20px" height="20px" viewBox="0 0 20 20" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
