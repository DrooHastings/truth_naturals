            </div>

            <style>
              #shopify-section-{{ section.id }} .ftr-sctn--soc-icns-lnk path {
                fill: {{ block.settings.soc_icons_color }};
              }

              @media screen and (min-width: 768px) {
                #shopify-section-{{ section.id }} .ftr-sctn--soc-icns-lnk path:hover {
                  fill: {{ block.settings.soc_icons_color_hover }};
                }
              }
            </style>

        {% endcase %}
      {% endfor %}
    </div>
  </div>

  {% if section.settings.show_payment_icons %}
    {% unless shop.enabled_payment_types == empty %}
      <ul id="ftr--payment-icons">
        {% for type in shop.enabled_payment_types %}
          {% if type != 'google_pay' %}
            <li class="ftr--payment-icon">
              {% capture icon_name %}payment-{{type}}{% endcapture %}
              {% include 'snippet-symbol' with icon_name %}
            </li>
          {% endif %}
        {% endfor %}
      </ul>
    {% endunless %}
  {% endif %}

  <p id="ftr-sctn--copyright">
    &copy; {{ "now" | date: "%Y" }}
    {% if section.settings.copyright != empty %}
        {{ section.settings.copyright }}
    {% else %}
      {{ shop.name }}
    {% endif %}
  </p>
</footer>
  {% comment %}<div class="sixteen columns mobile_only">
    <p class="mobile_only">
      {% if settings.show_multiple_currencies %}
        <label for="currencies">{{ 'layout.general.pick_a_currency' | t }}: </label>
        {% include 'currencies-switcher' %}
      {% endif %}
    </p>
  </div>{% endcomment %}
  <style>
    #ftr-sctn--content-wrap {
      background-color: {{ section.settings.color_bg }};
    }

    #shopify-section-{{ section.id }} .ftr-sctn--mn-ttl,
    #shopify-section-{{ section.id }} #ftr-sctn--srch-ttl {
      font-size: {{ section.settings.menu_title_size_mob }}px;
      color: {{ section.settings.menu_title_color }};
      font-weight: {{ section.settings.menu_title_weight }};
    }

    #shopify-section-{{ section.id }} .ftr-sctn--mn-lnk,
    #shopify-section-{{ section.id }} .ftr-sctn--inf-lnk {
      font-size: {{ section.settings.menu_item_size_mob }}px;
      color: {{ section.settings.menu_item_color }};
      font-weight: {{ section.settings.menu_item_weight }};
    }

    #shopify-section-{{ section.id }} #ftr--payment-icons {
      background-color: {{ section.settings.payment_icons_color_bg }};
    }

    #shopify-section-{{ section.id }} #ftr-sctn--copyright {
      font-size: {{ section.settings.copyright_size_mob }}px;
      color: {{ section.settings.copyright_color }};
